    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.11",
    "deep-diff": "^0.3.8",
    "draft-js": "^0.10.5",
    "draft-js-linkify-plugin": "^2.0.1",
    "draft-js-plugins-editor": "^2.0.4",
    "electron": "^2.0.0",
    "electron-builder": "20.7.0",
