  "dependencies": {
    "aes-js": "^3.1.1",
    "electron-mocha": "^6.0.2",
    "raf": "^3.4.0",
    "resize-observer-polyfill": "^1.5.0",
    "rimraf": "^2.6.1",
    "rrule": "^2.2.9",
    "uniqid": "^4.1.1"
  },
  "build": {
    "asar": true,
    "compression": "normal",
    "productName": "tasklist",
    "appId": "org.tasklist.app",
    "files": [
      "production/**/*",
      "!node_modules/electron-ga/node_modules${/*}",
      "!node_modules/retina-dom-to-image/bower_components${/*}",
      "!node_modules/electron",
      "!credentials"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./icon.ico"
    },
    "publish": {
      "provider": "s3",
      "bucket": "todoappupdates",
      "acl": "public-read"
    },
    "nsis": {
      "oneClick": false,
      "warningsAsErrors": false,
      "perMachine": true,
      "unicode": false,
      "allowToChangeInstallationDirectory": true,
      "menuCategory": true
    },
    "directories": {
      "output": "release"
    }
  }
}
