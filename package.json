{
  "name": "tasklist",
  "version": "1.5.9-master",
  "description": "tasklist",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "publish": "build --win --publish always",
    "pack": "build --win",
    "test:main": "electron-mocha ./tests/dist/main",
    "test:renderer:debug": "electron-mocha --renderer --debug ./tests/dist/renderer",
    "test:renderer": "electron-mocha --renderer ./tests/dist/renderer",
    "build": "start webpack --config webpack.main.js && webpack -w --config webpack.renderer.js",
    "build:tests:main": "start webpack -w --config webpack.tests.main.js",
    "build:tests:renderer": "start webpack -w --config webpack.tests.renderer.js",
    "production": "start webpack --config webpack.main.prod.js && webpack --config webpack.renderer.prod.js"
  },
  "repository": "",
  "keywords": [],
  "author": "Pixelbutler",
  "license": "",
