    "test:renderer:debug": "electron-mocha --renderer --debug ./tests/dist/renderer",
    "test:renderer": "electron-mocha --renderer ./tests/dist/renderer",
    "build": "start webpack -w --config webpack.main.js && webpack -w --config webpack.renderer.js",
    "build:tests:main": "start webpack -w --config webpack.tests.main.js",
    "build:tests:renderer": "start webpack -w --config webpack.tests.renderer.js",
    "production": "start webpack -w --config webpack.main.prod.js && webpack -w --config webpack.renderer.prod.js",

    "start:main": "webpack -w --config webpack.main.js",
    "start:renderer": "webpack -w --config webpack.renderer.js"
