{
  "name": "tasklist",
  "version": "1.2.9-master",
  "description": "tasklist",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w printer",
    "publish": "build --win --publish always",
    "pack": "build --win",
    "build": "start webpack -w --config webpack.config.main.js && webpack -w --config webpack.config.renderer.js",
    "production": "start webpack -w --config webpack.config.main.prod.js && webpack -w --config webpack.config.renderer.prod.js"
  },
  "repository": "",
  "keywords": [],
  "author": "Pixelbutler",
  "license": "",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^8.5.2",
    "@types/react": "^15.6.1",
    "@types/react-dom": "^15.5.2",
    "awesome-typescript-loader": "^3.2.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.7",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "electron": "1.8.2",
    "electron-builder": "19.54.0",
    "electron-prebuilt": "^1.4.13",
    "electron-publisher-s3": "^20.0.0",
    "electron-rebuild": "^1.7.3",
    "electron-winstaller": "^2.6.3",
    "html-webpack-plugin": "^2.30.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.17.0",
    "typescript": "^2.4.2",
    "uglify-es": "3.3.7",
    "uglifyjs-webpack-plugin": "1.1.8",
    "webpack": "^3.11.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "dom-to-image": "^2.6.0",
    "electron-ga": "^1.0.5",
    "electron-json-storage": "^4.0.2",
    "electron-log": "^2.2.13",
    "electron-updater": "2.20.1",
    "file-loader": "^0.11.2",
    "filereader": "^0.10.3",
    "font-loader": "^0.1.2",
    "fs": "0.0.1-security",
    "ical.js": "^1.2.2",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "mockdate": "^2.0.2",
    "moment": "^2.19.3",
    "node-machine-id": "^1.1.10",
    "path": "^0.12.7",
    "pouchdb-browser": "^6.4.2",
    "print-html-element": "^0.5.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "ramda": "^0.25.0",
    "random-word": "^2.0.0",
    "randomstring": "^1.1.5",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-audio-player": "^0.9.0",
    "react-color": "^2.13.8",
    "react-day-picker": "^7.0.4",
    "react-dom": "^15.6.1",
    "react-draggable": "^3.0.4",
    "react-input-autosize": "^2.1.2",
    "react-minimal-pie-chart": "^3.0.1",
    "react-redux": "^5.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-render": "^1.0.3",
    "react-test-renderer": "^15.6.1",
    "react-transition-group": "^2.2.1",
    "react-waypoint": "^7.3.2",
    "redux": "^3.7.2",
    "remote": "^0.2.6",
    "resize-observer-polyfill": "^1.5.0",
    "rimraf": "^2.6.1",
    "rx-dom": "^7.0.3",
    "rxjs": "^5.5.6",
    "typeface-roboto": "^0.0.45",
    "uniqid": "^4.1.1",
    "url": "^0.11.0"
  },
  "build": {
    "asar": true,
    "compression": "maximum",
    "productName": "tasklist",
    "appId": "org.tasklist.app",
    "files": [
      "production/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./icon.ico"
    },
    "publish": {
      "provider": "s3",
      "bucket": "todoappupdates",
      "acl": "public-read"
    },
    "nsis": {
      "oneClick": false,
      "warningsAsErrors": false,
      "perMachine": true,
      "unicode": false,
      "allowToChangeInstallationDirectory": true,
      "menuCategory": true
    },
    "directories": {
      "output": "release"
    }
  }
}
