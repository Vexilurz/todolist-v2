    "uniqid": "^5.2.0"
  },
  "build": {
    "asar": true,
    "compression": "normal",
    "productName": "tasklist",
    "appId": "org.tasklist.app",
    "files": [
      "production/**/*",
      "!node_modules/electron-ga/node_modules${/*}",
      "!node_modules/retina-dom-to-image/bower_components${/*}",
      "!node_modules/electron",
      "!credentials"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./icon.ico"
    },
    "publish": {
      "provider": "s3",
      "bucket": "todoappupdates",
      "acl": "public-read"
    },
    "nsis": {
      "oneClick": false,
      "warningsAsErrors": false,
      "perMachine": true,
      "unicode": false,
      "allowToChangeInstallationDirectory": true,
      "menuCategory": true
    },
    "directories": {
      "output": "release"
    }
  }
