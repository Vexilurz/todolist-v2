{
  "name": "tasklist",
  "version": "1.7.5-master",
  "description": "tasklist",
  "main": "main.js",
  "scripts": {
    "start:old": "electron .",
    "publish": "build --win --publish always",
    "pack:old": "build --win",
    "test:main": "electron-mocha ./tests/dist/main",
    "test:renderer:debug": "electron-mocha --renderer --debug ./tests/dist/renderer",
    "test:renderer": "electron-mocha --renderer ./tests/dist/renderer",
    "build:old": "start webpack -w --config webpack.main.js && webpack -w --config webpack.renderer.js",
    "build:tests:main": "start webpack -w --config webpack.tests.main.js",
    "build:tests:renderer": "start webpack -w --config webpack.tests.renderer.js",
    "production": "start webpack -w --config webpack.main.prod.js && webpack -w --config webpack.renderer.prod.js",
    "start:main": "webpack -w --config webpack.main.js",
    "start:renderer": "webpack -w --config webpack.renderer.js",
    "start:dev": "cross-env START_HOT=1 webpack-dev-server --config webpack.renderer.dev.config.js",
    "------": "----------",
    "build": "npx webpack --env.NODE_ENV=development --env.target=render && npx webpack --env.NODE_ENV=development --env.target=main",
    "build-prod": "npx webpack --env.NODE_ENV=production --env.target=render && npx webpack --env.NODE_ENV=production --env.target=main",
    "server": "npx webpack-dev-server --env.NODE_ENV=development --env.target=render --hot",
    "start": "npx webpack --env.NODE_ENV=development --env.target=main && npx electron ./dist/.",
    "pack": "npm run build-prod && npx electron-packager ./dist --out ./pack --overwrite"
  },
